{{> navbar}}
<br/>

<h1>Listado de productos</h1>



<table class="table">
  <thead>
    <tr>
      <th scope="col">ID</th>
      <th scope="col">Nombre</th>
      <th scope="col">Precio</th>
      <th scope="col">Img</th>
    </tr>
  </thead>
  <tbody>
        {{#each arrayCompleto}}
            <th scope="row">{{this.id}}</th>
                <td> {{this.title}}</td>
                <td>{{this.price}}</td>
                <td><img src="{{this.thumbnail}}" class="product-img"/></td>
            </tr>
        {{/each}}
  </tbody>
</table>

<br/>
{{> footer}}
<br/>